{
  "name": "@sfajs/cli",
  "version": "0.1.2",
  "description": "sfajs 的命令行脚手架工具",
  "homepage": "https://github.com/sfajs/cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/sfajs/cli"
  },
  "keywords": [
    "sfa",
    "sfajs"
  ],
  "bugs": {
    "url": "https://github.com/sfajs/cli/issues",
    "email": "support@hal.wang"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "sh scripts/build.sh",
