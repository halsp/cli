  "dependencies": {
    "@sfajs/cli-common": "^0.2.5",
    "@sfajs/core": "^1.4.7",
    "@sfajs/inject": "^1.1.0",
    "@sfajs/pipe": "^1.1.6",
    "chalk": "^4.1.2",
    "child_process": "^1.0.2",
    "chokidar": "^3.5.3",
    "commander": "^9.3.0",
    "figlet": "^1.5.2",
    "ignore-walk": "^5.0.1",
    "inquirer": "^8.2.4",
    "ora": "5.4.1",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "shelljs": "^0.8.5",
    "tree-kill": "^1.2.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
