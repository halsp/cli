    "ts-node": {
      "optional": true
    },
    "@sfajs/cli-common": {
      "optional": true
    }
  },
  "bin": {
    "sfa": "bin/index.js"
  },
  "files": [
    "/.github/*",
    "/bin/*",
    "/dist/*",
    "/env/*",
    "/scripts/*",
    "/src/*",
    "/template/*",
    "/test/*",
    ".eslintignore",
    ".eslintrc.js",
    ".gitignore",
    "CODE_OF_CONDUCT.md",
    "CONTRIBUTING.md",
    "jest.config.js",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ]
}
