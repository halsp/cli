{
  "name": "@sfajs/cli",
  "version": "0.0.1",
  "description": "sfajs 的命令行脚手架工具",
  "homepage": "https://github.com/sfajs/cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/sfajs/cli"
  },
  "keywords": [
    "sfa",
    "sfajs"
  ],
  "bugs": {
    "url": "https://github.com/sfajs/cli/issues",
    "email": "support@hal.wang"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "sh scripts/build.sh",
    "publish:beta": "npm run build && npm publish --tag=beta --access=public",
    "test": "npm run build && jest",
    "tests": "sh scripts/tests.sh",
    "lint": "eslint src test --ext .ts",
    "dev": "npm run build && npm uninstall @sfajs/cli -g && npm link"
  },
  "author": "hal-wang",
  "license": "MIT",
  "dependencies": {
    "@sfajs/core": "^1.4.2",
    "@sfajs/http": "^1.1.0",
    "@sfajs/inject": "^1.0.6",
    "@sfajs/pipe": "^1.1.3",
    "chalk": "^4.1.2",
    "child_process": "^1.0.2",
    "commander": "^9.2.0",
    "figlet": "^1.5.2",
    "inquirer": "^8.2.4",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.4",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "codecov": "^3.8.3",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  },
  "bin": {
    "sfa": "bin/index.js"
  },
  "files": [
    "/.github/*",
    "/bin/*",
    "/dist/*",
    "/scripts/*",
    "/src/*",
    "/test/*",
    ".eslintignore",
    ".eslintrc.js",
    ".gitignore",
    "jest.config.js",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ]
}
